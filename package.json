{
  "name": "abell-renderer",
  "version": "0.1.13",
  "description": "A wrapper arround Mustache that adds some additional features and some syntatic sugar required for abell",
  "main": "dist/index.js",
  "bin": {
    "abell-renderer": "dist/bin.js"
  },
  "funding": {
    "type": "patreon",
    "url": "https://patreon.com/saurabhdaware"
  },
  "scripts": {
    "test": "mocha --recursive \"./tests/*.js\"",
    "build": "node build/build.main.js",
    "dev": "node src/example/example.js",
    "dev:cli": "node src/bin.js build --input src/example/templates/cli_example.abell --output src/example/out/cli_example.html --allow-require",
    "dev:cli-dir": "node src/bin.js build --input src/example/templates/loop_example --output src/example/out/loop_example --allow-require",
    "cli": "npm run dev:cli",
    "eslint": "eslint .",
    "prettier": "prettier --write \"**/*.js\"",
    "prepublishOnly": "npm run eslint && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abelljs/abell-renderer.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abelljs/abell-renderer/issues"
  },
  "homepage": "https://github.com/abelljs/abell-renderer#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fs-extra": "^9.0.0",
    "husky": "^4.2.5",
    "mocha": "^7.1.2",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && eslint . --fix && npm run eslint"
    }
  }
}